{
  "name": "chiliz-mcp",
  "version": "1.0.0",
  "description": "Advanced MCP server for Chiliz blockchain with Brazilian tokens, FanX DEX, analytics & transactions",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "chiliz-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": ["mcp", "chiliz", "blockchain", "fan-tokens", "sports", "brazil", "fanx", "dex", "defi"],
  "author": "Bruno Pessoa <bruno.pessoa@chiliz.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoPessoa22/chiliz-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/BrunoPessoa22/chiliz-mcp/issues"
  },
  "homepage": "https://chiliz-mcp.vercel.app",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "axios": "^1.7.2",
    "ethers": "^6.13.1",
    "node-cache": "^5.1.2",
    "dotenv": "^16.4.5",
    "solc": "^0.8.21",
    "zod": "^3.22.4",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/jest": "^29.5.12",
    "typescript": "^5.5.3",
    "tsx": "^4.16.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "@typescript-eslint/parser": "^7.16.0",
    "@typescript-eslint/eslint-plugin": "^7.16.0"
  }
}